# Environment variables configuration
# Command: cp .env.example .env

# OpenAI API Key
OPENAI_API_KEY=sk-your-openai-api-key-here

# OpenAI API base URL
OPENAI_BASE_URL=https://api.openai.com/v1

# Langfuse Secret Key
LANGFUSE_SECRET_KEY=sk-your-langfuse-secret-key-here

# Langfuse Public Key
LANGFUSE_PUBLIC_KEY=pk-your-langfuse-public-key-here

# Langfuse Host
LANGFUSE_HOST=https://langfuse.example.com

# Available LLM models (comma-separated list)
AVAILABLE_LLMS=gpt-4-0613,gpt-3.5-turbo-1106,claude-3-opus-20240229,claude-3-sonnet-20240229

# Default models
DEFAULT_CLEANING_MODEL=gpt-4-0613
DEFAULT_QA_MODEL=gpt-4-0613

# Default target language
DEFAULT_TARGET_LANGUAGE=en

# Backend API base URL
VITE_API_BASE_URL=http://localhost:8000

# OpenWebUI
OWUI_URL=

OWUI_TOKEN=
